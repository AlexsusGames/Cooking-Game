    \u0441\u0432\u043E\u044E \u0436\u0438\u0437\u043D\u044C. \u0421\u043F\u0443\u0441\u0442\u044F
    \u0432\u0440\u0435\u043C\u044F, \u0440\u0430\u043A \u0432\u043E\u0448\u0435\u043B
    \u0432 \u0440\u0435\u043C\u0438\u0441\u0441\u0438\u044E."
  - "\u041E\u043D \u0432\u0435\u0440\u043D\u0443\u043B\u0441\u044F \u043A \u0441\u0432\u043E\u0435\u043C\u0443
    \u043B\u044E\u0431\u0438\u043C\u043E\u043C\u0443 \u0434\u0435\u043B\u0443 \u2014
    \u043A\u0430\u0444\u0435, \u043A\u043E\u0442\u043E\u0440\u043E\u0435 \u043E\u043D
    \u043A\u043E\u0433\u0434\u0430-\u0442\u043E \u043F\u043E\u0441\u0442\u0440\u043E\u0438\u043B
    \u0441 \u043D\u0443\u043B\u044F. \u0422\u0435\u043F\u043B\u043E \u0437\u0430
    \u0431\u0430\u0440\u043D\u043E\u0439 \u0441\u0442\u043E\u0439\u043A\u043E\u0439
    \u0438 \u0437\u0430\u043F\u0430\u0445 \u0441\u0432\u0435\u0436\u0435\u0437\u0430\u0432\u0430\u0440\u0435\u043D\u043D\u043E\u0433\u043E
    \u043A\u043E\u0444\u0435 \u0441\u043D\u043E\u0432\u0430 \u0441\u0442\u0430\u043B\u0438
    \u0447\u0430\u0441\u0442\u044C\u044E \u0435\u0433\u043E \u0436\u0438\u0437\u043D\u0438."
  - "\u0421\u0430\u0440\u0430, \u0435\u0433\u043E \u0434\u043E\u0447\u044C, \u043F\u0440\u043E\u0448\u043B\u0430
    \u0447\u0435\u0440\u0435\u0437 \u0441\u0432\u043E\u0451 \u0441\u043E\u0431\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0435
    \u0438\u0441\u043F\u044B\u0442\u0430\u043D\u0438\u0435. \u041F\u043E\u043C\u043E\u0433\u0430\u044F
    \u043E\u0442\u0446\u0443 \u0432 \u043A\u0430\u0444\u0435, \u043E\u043D\u0430
    \u043E\u0441\u043E\u0437\u043D\u0430\u043B\u0430, \u043D\u0430\u0441\u043A\u043E\u043B\u044C\u043A\u043E
    \u0442\u044F\u0436\u0435\u043B \u0442\u0440\u0443\u0434, \u043A\u0430\u043A \u0441\u043B\u043E\u0436\u043D\u043E
    \u0443\u043F\u0440\u0430\u0432\u043B\u044F\u0442\u044C \u0431\u0438\u0437\u043D\u0435\u0441\u043E\u043C
    \u0438 \u0443\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u0442\u044C \u0435\u0433\u043E
    \u043D\u0430 \u043F\u043B\u0430\u0432\u0443."
  - "\u041E\u043D\u0430 \u0443\u0432\u0438\u0434\u0435\u043B\u0430 \u043D\u0435 \u0442\u043E\u043B\u044C\u043A\u043E
    \u0435\u0433\u043E \u0441\u0442\u0440\u0430\u0434\u0430\u043D\u0438\u044F, \u043D\u043E
    \u0438 \u0435\u0433\u043E \u0441\u0438\u043B\u0443, \u0438 \u044D\u0442\u043E
    \u0432\u0434\u043E\u0445\u043D\u043E\u0432\u0438\u043B\u043E \u0435\u0451. \u041D\u0430\u043A\u043E\u043F\u0438\u0432
    \u0434\u0435\u043D\u044C\u0433\u0438 \u0437\u0430 \u0432\u0440\u0435\u043C\u044F
    \u0440\u0430\u0431\u043E\u0442\u044B \u0441 \u043E\u0442\u0446\u043E\u043C, \u0421\u0430\u0440\u0430
    \u043F\u043E\u0441\u0442\u0443\u043F\u0438\u043B\u0430 \u0432 \u043A\u043E\u043B\u043B\u0435\u0434\u0436,
    \u0440\u0435\u0448\u0438\u0432 \u0438\u0437\u0443\u0447\u0430\u0442\u044C \u044D\u043A\u043E\u043D\u043E\u043C\u0438\u043A\u0443. "
  - "\u041E\u043D\u0430 \u043C\u0435\u0447\u0442\u0430\u043B\u0430 \u043E\u0434\u043D\u0430\u0436\u0434\u044B
    \u0440\u0430\u0437\u0432\u0438\u0442\u044C \u0441\u0435\u043C\u0435\u0439\u043D\u043E\u0435
    \u043A\u0430\u0444\u0435 \u0438 \u043F\u0440\u0435\u0432\u0440\u0430\u0442\u0438\u0442\u044C
    \u0435\u0433\u043E \u0432 \u043D\u0435\u0447\u0442\u043E \u0431\u043E\u043B\u044C\u0448\u0435\u0435,
    \u0447\u0435\u043C \u043F\u0440\u043E\u0441\u0442\u043E \u043C\u0435\u0441\u0442\u043E
    \u0434\u043B\u044F \u0432\u0441\u0442\u0440\u0435\u0447\u0438 \u0441\u0442\u0430\u0440\u044B\u0445
    \u0434\u0440\u0443\u0437\u0435\u0439. "
  - "\u0421\u0430\u0440\u0430 \u0445\u043E\u0442\u0435\u043B\u0430, \u0447\u0442\u043E\u0431\u044B
    \u044D\u0442\u043E \u043A\u0430\u0444\u0435 \u0441\u0442\u0430\u043B\u043E \u0441\u0438\u043C\u0432\u043E\u043B\u043E\u043C
    \u0441\u0442\u043E\u0439\u043A\u043E\u0441\u0442\u0438, \u0441\u0435\u043C\u044C\u0438
    \u0438 \u0432\u0442\u043E\u0440\u043E\u0439 \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u0438."
  - "\u0418\u0441\u0442\u043E\u0440\u0438\u044F \u0414\u044D\u043D\u0430 \u0438 \u0421\u0430\u0440\u044B
    \u043D\u0435 \u043F\u0440\u043E\u0441\u0442\u043E \u0437\u0430\u0432\u0435\u0440\u0448\u0438\u043B\u0430\u0441\u044C
    \u0445\u043E\u0440\u043E\u0448\u043E. \u042D\u0442\u043E \u0438\u0441\u0442\u043E\u0440\u0438\u044F
    \u043E \u0442\u043E\u043C, \u043A\u0430\u043A \u0434\u0430\u0436\u0435 \u0432
    \u0441\u0430\u043C\u044B\u0435 \u0442\u0451\u043C\u043D\u044B\u0435 \u043C\u043E\u043C\u0435\u043D\u0442\u044B
    \u043C\u043E\u0436\u043D\u043E \u043D\u0430\u0439\u0442\u0438 \u0441\u0432\u0435\u0442. "
  - "\u0422\u0435\u043F\u0435\u0440\u044C \u043E\u043D\u0438 \u0432\u043C\u0435\u0441\u0442\u0435,
    \u0441 \u043D\u043E\u0432\u044B\u043C\u0438 \u043D\u0430\u0434\u0435\u0436\u0434\u0430\u043C\u0438
