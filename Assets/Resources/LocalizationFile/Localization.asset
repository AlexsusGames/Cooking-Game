      \u0437\u0430\u043B\u0438\u0448\u0438\u0442\u0438\u0441\u044F \u043D\u0430 \u0437\u0432'\u044F\u0437\u043A\u0443,
      \u0430\u043B\u0435 \u0437\u0430\u0440\u0430\u0437 \u0457\u0439 \u0441\u043F\u0440\u0430\u0432\u0434\u0456
      \u043F\u043E\u0442\u0440\u0456\u0431\u043D\u0430 \u043F\u0456\u0434\u0442\u0440\u0438\u043C\u043A\u0430."
